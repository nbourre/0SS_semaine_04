<Window x:Class="wpf_images_multiples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_images_multiples"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400">
    <Grid Margin="12">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Grid.Column="0" Margin="6" Source="images/ayeaye.jpg"/>
        <Image Grid.Row="1" Grid.Column="0" Margin="6" Source="images/cat.jpg"/>
        <Image Grid.Row="0" Grid.Column="1" Margin="6" Source="images/mr_t.jpg"/>
        <Image Grid.Row="1" Grid.Column="1" Margin="6" Source="images/obama.jpg"/>
        
    </Grid>
</Window>
